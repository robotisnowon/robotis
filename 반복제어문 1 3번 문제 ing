#include <stdio.h>
int main(){
	int array[100];
	int a, count = 0; 
	int sum;
	int avg; 
	while(1){
		scanf("%d", &a);
		if(a>100){
			break;
		} else {
			array[count]=a;
			count++;
		}
	} 
		for(int i=0; i<=100; i++){ 
			
			printf("%d", i);
	
		}
	
		for(int j=0; j<=100; j++){
		
			printf("%d", j);
		}
}
